{
  "name": "cloudstack-ui",
  "version": "1.411.23",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "hmr": "npm run build-themes && ng serve --configuration hmr",
    "build-themes": "sh ./scripts/generate-themes.sh",
    "build": "npm run build-themes && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "test": "ng test --code-coverage --watch=false",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:ci": "mkdir -p reports; ng lint cloudstack-ui --format checkstyle > reports/checkstyle-result.xml",
    "e2e": "ng e2e",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.8",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.8",
    "@angular/core": "6.1.8",
    "@angular/forms": "6.1.8",
    "@angular/http": "6.1.8",
    "@angular/material": "6.4.7",
    "@angular/platform-browser": "6.1.8",
    "@angular/platform-browser-dynamic": "6.1.8",
    "@angular/platform-server": "6.1.8",
    "@angular/router": "6.1.8",
    "@mdi/font": "^2.7.94",
    "@ng-idle/core": "6.0.0-beta.3",
    "@ng-idle/keepalive": "6.0.0-beta.3",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "ajv": "^6.5.2",
    "ajv-errors": "^1.0.0",
    "ajv-keywords": "^3.2.0",
    "chart.js": "2.7.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ng-dynamic-component": "^3.0.0",
    "ng2-dragula": "1.5.0",
    "ngx-clipboard": "^11.1.7",
    "rxjs": "^6.3.2",
    "showdown": "^1.8.4",
    "uuid": "^3.1.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "^6.2.1",
    "@angular/compiler": "6.1.8",
    "@angular/compiler-cli": "6.1.8",
    "@angular/language-service": "6.1.8",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/node": "^8.0.16",
    "@types/showdown": "^1.7.1",
    "codelyzer": "~4.2.1",
    "conventional-changelog": "^2.0.3",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "^3.0.2",
    "husky": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
