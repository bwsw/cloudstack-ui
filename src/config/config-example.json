{
  "defaultDomain": "domain",
  "sessionRefreshInterval": 60,
  "apiDocLink": "https://cloudstack.apache.org/api/apidocs-4.11/",
  "extensions": {
    "webShell": true,
    "pulse": true,
    "vmLogs": true,
    "resourceLimits": true
  },
  "vmLogs": {
    "autoUpdateRefreshFrequency": 1,
    "autoUpdateRequestedInterval": 30
  },
  "resourceLimits": {
    "realm": "resource-limits",
    "env": "prod"
  },
  "vmColors": [
    { "value": "#F44336" },
    { "value": "#E91E63" },
    { "value": "#9C27B0" },
    { "value": "#673AB7" },
    { "value": "#3F51B5" }
  ],
  "vmSnapLimit": {
    "enable": false,
    "snapshotsLimit": 0
  },
  "securityGroupTemplates": [
    {
      "id": "templateTCP",
      "name": "TCP Permit All",
      "description": "Permits all TPC traffic",
      "preselected": false,
      "ingressrule": [
        {
          "ruleid": "templateTCP-rule-ingress",
          "protocol": "tcp",
          "startport": 1,
          "endport": 65535,
          "cidr": "0.0.0.0/0"
        }
      ],
      "egressrule": [
        {
          "ruleid": "templateTCP-rule-egress",
          "protocol": "tcp",
          "startport": 1,
          "endport": 65535,
          "cidr": "0.0.0.0/0"
        }
      ]
    },
    {
      "id": "templateICMP",
      "name": "ICMP Permit Egress",
      "description": "Permits all egress ICMP traffic",
      "preselected": true,
      "ingressrule": [],
      "egressrule": [
        {
          "ruleid": "templateICMP-rule-egress",
          "icmpcode": -1,
          "icmptype": -1,
          "protocol": "icmp",
          "cidr": "0.0.0.0/0"
        }
      ]
    }
  ],
  "imageGroups": [
    {
      "id": "id-234",
      "translations": {
        "ru": "Имя группы",
        "en": "Group Name"
      }
    }
  ],
  "defaultFirstDayOfWeek": 0,
  "defaultInterfaceLanguage": "en",
  "defaultTimeFormat": "hour24",
  "defaultTheme": "blue-red",
  "sessionTimeout": 30,
  "autoGenerateUserKeys": true,
  "customComputeOfferingParameters": [
    {
      "offeringId": "REPLACE_WITH_ID",
      "cpunumber": {
        "min": 2,
        "max": 8,
        "value": 4
      },
      "cpuspeed": {
        "min": 1000,
        "max": 3000,
        "value": 1500
      },
      "memory": {
        "min": 512,
        "max": 8192,
        "value": 512
      }
    }
  ],
  "defaultComputeOffering": [
    {
      "zoneId": "REPLACE_WITH_ID",
      "offeringId": "REPLACE_WITH_ID"
    }
  ],
  "offeringCompatibilityPolicy": {
    "offeringChangePolicy": "exactly-match",
    "offeringChangePolicyIgnoreTags": ["t1"]
  },
  "computeOfferingClasses": [
    {
      "id": "classId-1",
      "name": {
        "ru": "Имя класса - 1",
        "en": "Class name - 1"
      },
      "description": {
        "ru": "Описание класса - 1",
        "en": "Class description - 1"
      },
      "computeOfferings": ["REPLACE_WITH_ID"]
    },
    {
      "id": "classId-2",
      "name": {
        "ru": "Имя класса - 2",
        "en": "Class name - 2"
      },
      "description": {
        "ru": "Описание класса - 2",
        "en": "Class description - 2"
      },
      "computeOfferings": ["REPLACE_WITH_ID"]
    }
  ],
  "serviceOfferingAvailability": {
    "filterOfferings": true,
    "zones": {
      "REPLACE_WITH_ID": {
        "diskOfferings": ["REPLACE_WITH_ID"],
        "computeOfferings": ["REPLACE_WITH_ID"]
      }
    }
  },
  "defaultSecurityGroupName": {
    "en": "default name",
    "ru": "имя по умолчанию"
  }
}
