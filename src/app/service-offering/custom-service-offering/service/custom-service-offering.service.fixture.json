{
  "restrictionsTests": {
    "allParamsAreSpecifiedAndConformToRestrictions": {
      "config": {
        "customOfferingRestrictions": {
          "zone1": {
            "cpuNumber": {
              "min": 1,
              "max": 4
            },
            "cpuSpeed": {
              "min": 1000,
              "max": 2000
            },
            "memory": {
              "min": 32,
              "max": 8192
            }
          }
        }
      },
      "resources": {
        "available": {
          "cpus": 8,
          "memory": 16384
        }
      },
      "expected": {
        "zone1": {
          "cpuNumber": {
            "min": 1,
            "max": 4
          },
          "cpuSpeed": {
            "min": 1000,
            "max": 2000
          },
          "memory": {
            "min": 32,
            "max": 8192
          }
        }
      }
    },
