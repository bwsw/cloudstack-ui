{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "vmColors",
  "type": "array",
  "uniqueItems": true,
  "items": {
    "type": "object",
    "required": ["value"],
    "additionalProperties": false,
    "properties": {
      "value": {
        "type": "string",
        "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
      }
    }
  }
}
